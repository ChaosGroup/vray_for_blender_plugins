{
    "Desciption": "", 
    "ID": "LightRectangle", 
    "Name": "Rectangle", 
    "Parameters": [
        {
            "attr": "enabled", 
            "default": true, 
            "desc": "true if the light is casting light (turned on) and false otherwise; it only makes sense to use this parameter if it is animated, or if another object depends on this node but it must not be rendered", 
            "type": "BOOL"
        }, 
        {
            "attr": "transform", 
            "default": null, 
            "desc": "", 
            "type": "TRANSFORM"
        }, 
        {
            "attr": "color", 
            "default": [
                1, 
                1, 
                1
            ], 
            "desc": "Color of the light", 
            "type": "COLOR"
        }, 
        {
            "attr": "color_tex", 
            "default": [
                1.0, 
                1.0, 
                1.0
            ], 
            "desc": "A color texture that if present will override the color parameter", 
            "name": "Color", 
            "type": "TEXTURE",
            "options": [
                "LINKED_ONLY"
            ]
        }, 
        {
            "attr": "shadows", 
            "default": true, 
            "desc": "true if the light casts shadows and false otherwise", 
            "type": "BOOL"
        }, 
        {
            "attr": "shadowColor", 
            "default": [
                0, 
                0, 
                0
            ], 
            "desc": "The shadow color. Anything but black is not physically accurate", 
            "type": "COLOR"
        }, 
        {
            "attr": "shadowColor_tex", 
            "default": [
                0.0, 
                0.0, 
                0.0
            ], 
            "desc": "A color texture that if present will override the shadowColor parameter", 
            "type": "TEXTURE",
            "options": [
                "LINKED_ONLY"
            ]
        }, 
        {
            "attr": "shadowBias", 
            "default": 0, 
            "desc": "Shadow offset from the surface. Helps to prevent polygonal shadow artifacts on low-poly surfaces", 
            "type": "FLOAT", 
            "ui": {
                "max": 1048576, 
                "min": -1048576, 
                "soft_max": 64, 
                "soft_min": 0
            }
        }, 
        {
            "attr": "photonSubdivs", 
            "default": 500, 
            "desc": "", 
            "type": "INT", 
            "ui": {
                "max": 1048576, 
                "min": -1048576, 
                "soft_max": 64, 
                "soft_min": 0
            }
        }, 
        {
            "attr": "causticSubdivs", 
            "default": 1000, 
            "desc": "", 
            "type": "INT", 
            "ui": {
                "max": 1048576, 
                "min": -1048576, 
                "soft_max": 64, 
                "soft_min": 0
            }
        }, 
        {
            "attr": "diffuseMult", 
            "default": 1, 
