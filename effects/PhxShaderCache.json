            "default": ""
        },
        {
            "attr": "smooth_similarity",
            "desc": "",
            "type": "FLOAT_LIST",
            "default": ""
        },
        {
            "attr": "smooth_randomize",
            "desc": "",
            "type": "FLOAT_LIST",
            "default": ""
        },
        {
            "attr": "free_mem_detect",
            "desc": "internally used",
            "type": "INT",
            "default": 0
        }
