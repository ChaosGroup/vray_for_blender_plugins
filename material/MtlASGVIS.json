        {
            "attr": "displace_tex_mult",
            "desc": "displacement multiplier ( displace amount )",
            "type": "FLOAT",
            "default": 1.0
        },
        {
            "attr": "displace_shift",
            "desc": "a constant value added to the displacement",
            "type": "FLOAT",
            "default": 0.0
        },
        {
            "attr": "displace_water_level",
