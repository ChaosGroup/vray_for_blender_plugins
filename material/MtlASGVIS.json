            "elements_count": 0,
            "default": null
        },
        {
            "attr": "double_sided",
            "desc": "1 to make the material double-sided",
            "type": "BOOL",
            "default": true
        },
        {
            "attr": "disable_volume_fog",
            "desc": "Set this flag to disable any volumetric effects that sub-brdfs might need",
            "type": "BOOL",
            "default": false
        },
        {
            "attr": "bump_on",
            "desc": "true if we want to turn on bump mapping",
            "type": "BOOL",
            "default": false
        },
        {
            "attr": "bump_tex",
            "desc": "Bump texture, can be float or color",
