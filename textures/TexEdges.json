            {
                "layout": "SEPARATOR"
            },
            {
                "attrs": [
                    {
                        "name": "show_hidden_edges"
                    },
                    {
                        "name": "show_subtriangles"
                    }
                ],
                "layout": "ROW"
            },
            {
                "attrs": [
                    { "name": "raytraced" },
                    { "name": "raytraced_mode" },
                    { "name": "raytraced_consider_same_object_only" }
                ],
                "layout": "COLUMN"
            }
        ]
    }
