{
    "Desciption": "",
    "ID": "TexEdges",
    "Name": "Edges",
    "Parameters": [
        {
            "attr": "edges_tex",
            "default": [
                1,
                1,
                1
            ],
            "desc": "",
            "type": "TEXTURE",
            "ui": {
                "display_name": "Edges color"
            }
        },
        {
            "attr": "bg_tex",
            "default": [
                0,
                0,
                0
            ],
            "desc": "",
            "type": "TEXTURE",
            "ui": {
                "display_name": "Background color"
            }
        },
        {
            "attr": "width_type",
            "default": "1",
            "desc": "Width Type",
            "items": [
                [
                    "0",
                    "World",
                    ""
                ],
                [
                    "1",
                    "Pixels",
                    ""
                ]
            ],
            "type": "ENUM",
            "ui": {
                "display_name": "Width type"
            }
        },
        {
            "attr": "pixel_width",
            "default": 1,
            "desc": "",
            "name": "Width",
            "type": "FLOAT_TEXTURE",
            "ui": {
                "max": 1048576,
                "min": -1048576,
                "soft_max": 64,
                "soft_min": 0,
                "display_name": "Pixel width",
                "enabled": true
            }
        },
        {
            "attr": "show_hidden_edges",
            "default": false,
            "desc": "",
            "type": "BOOL",
            "ui": {
                "display_name": "Show hidden edges"
            }
