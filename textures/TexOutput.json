{
    "Desciption": "",
    "ID": "TexOutput",
    "Name": "Output",
    "Subtype": "UTILITY",
    "Parameters": [
        {
            "attr": "compatibility_with",
            "default": 0,
            "desc": "This is used to differentiate between textures exported from different applications",
            "type": "INT",
            "ui": {
                "max": 1048576,
                "min": -1048576,
                "soft_max": 64,
                "soft_min": 0,
                "rollout": "Common"
            }
        },
        {
            "attr": "alpha_from_intensity",
            "desc": "If 2 - the alpha is forced to 1.0f; 1 - the resulting alpha is the color intensity (if compatibility_with is 0) or the color luminance (if compatibility_with is 1); 0 - the alpha is taken from the bitmap alpha",
            "type": "ENUM",
            "items": [
                [
                    "0",
                    "From Texture",
                    ""
                ],
                [
                    "1",
                    "From Intensity",
                    ""
                ],
                [
                    "2",
                    "Force Opaque",
                    ""
                ]
            ],
            "default": "1"
        },
        {
            "attr": "invert",
            "default": false,
            "desc": "If true, the resulting texture color will be inverted",
            "type": "BOOL"
        },
        {
            "attr": "invert_alpha",
            "default": true,
            "desc": "If true and invert is on, the resulting texture alpha will be inverted too. If false, just the color will be inverted",
            "type": "BOOL",
            "ui": {
                "enabled": true
            }
        },
        {
            "attr": "color_mult",
            "name": "RGB Level",
            "desc": "A multiplier for the texture color",
            "type": "FLOAT",
            "default": 1.0,
            "options": [
                "EXPORT_AS_ACOLOR"
            ]
        },
        {
            "attr": "color_offset",
            "name": "RGB Offset",
            "desc": "An additional offset for the texture color",
            "type": "FLOAT",
            "default": 1.0,
            "options": [
                "EXPORT_AS_ACOLOR"
            ]
        },
        {
            "attr": "alpha_mult",
            "default": 1,
            "desc": "A multiplier for the texture alpha",
            "type": "FLOAT_TEXTURE",
            "ui": {
                "max": 1048576,
                "min": -1048576,
                "soft_max": 64,
                "soft_min": 0
            }
        },
        {
            "attr": "alpha_offset",
            "default": 0,
            "desc": "An additional offset for the texture alpha",
            "type": "FLOAT_TEXTURE",
            "ui": {
                "max": 1048576,
                "min": -1048576,
                "soft_max": 64,
                "soft_min": 0
            }
        },
        {
            "attr": "nouvw_color",
            "default": [
                0.5,
                0.5,
                0.5,
                1
            ],
            "desc": "The color when there are no valid uvw coordinates",
            "type": "TEXTURE",
            "ui": {
                "display_name": "No UVs Color"
            }
        },
        {
            "attr": "color",
            "default": [
                1.0,
                1.0,
                1.0
            ],
            "desc": "The resulting color",
            "type": "OUTPUT_TEXTURE"
        },
        {
            "attr": "out_transparency",
            "default": [
                1.0,
                1.0,
                1.0
            ],
            "desc": "The resulting transparency",
            "type": "OUTPUT_TEXTURE"
        },
        {
            "attr": "out_alpha",
            "default": 1.0,
            "desc": "The resulting alpha",
            "type": "OUTPUT_FLOAT_TEXTURE"
        },
        {
            "attr": "out_intensity",
            "default": 1.0,
            "desc": "The resulting intensity",
            "type": "OUTPUT_FLOAT_TEXTURE",
            "ui": {
                "rollout": "<EndRollout>"
            }
        },
        {
            "attr": "texmap",
            "name": "Input",
            "default": [
                1,
                1,
                1,
                1
            ],
            "desc": "The texture",
            "type": "TEXTURE"
        },
        {
            "attr": "bump_amount",
            "default": 1,
            "desc": "Bump amount if the texture is used for bump mapping",
            "type": "FLOAT",
            "ui": {
                "max": 1048576,
                "min": -1048576,
                "soft_max": 64,
                "soft_min": 0
            }
        },
        {
            "attr": "output_mult",
            "name": "Output Amount",
            "desc": "A multiplier for the output color",
            "type": "FLOAT",
            "default": 1.0,
            "options": [
                "EXPORT_AS_ACOLOR"
            ]
        },
        {
            "attr": "output_clamp",
            "name": "Clamp",
            "desc": "Clamp the output in [0.0,1.0] range",
            "type": "BOOL",
            "default": false
        }
    ],
    "Type": "TEXTURE",
    "Widget": {
        "node_widgets": [
            {
                "layout": "COLUMN",
                "attrs": [
                    { "name": "invert" },
                    { "name": "output_clamp" },
                    { "name": "alpha_from_intensity", "label": "Alpha" },
                    { "name": "color_mult" },
                    { "name": "color_offset" },
                    { "name": "output_mult" }
                ]
            }
        ],
        "widgets": []
    }
}