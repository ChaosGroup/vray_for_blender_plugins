            "type": "FLOAT_TEXTURE", 
            "ui": {
                "max": 1048576, 
                "min": -1048576, 
                "soft_max": 64, 
                "soft_min": 0
            }
        }, 
        {
            "attr": "blend_output", 
            "default": 1.0, 
