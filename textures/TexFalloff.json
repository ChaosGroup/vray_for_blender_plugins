                    "View Y", 
                    ""
                ], 
                [
                    "3", 
                    "Explicit", 
                    ""
                ], 
                [
                    "4", 
                    "Local X", 
                    ""
                ], 
                [
                    "5", 
                    "Local Y", 
                    ""
                ], 
                [
                    "6", 
                    "Local Z", 
                    ""
                ], 
                [
                    "7", 
                    "World X", 
                    ""
                ], 
                [
                    "8", 
                    "World Y", 
                    ""
                ], 
                [
                    "9", 
                    "World Z", 
                    ""
                ]
            ], 
            "type": "ENUM"
        }, 
